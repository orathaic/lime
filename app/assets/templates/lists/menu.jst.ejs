<div class="list-menu app-drop-menu">
  <div class="app-drop-button"></div>
  <menu>

    <button class="edit-list">Edit list</button>

    <button class="complete-list">
      <% if(list.get('completed')){ %>
        Mark as uncompleted
      <% } else { %>
        Mark as completed
      <% }; %>
    </button>

    <button class="archive-list">
      <% if(list.get('archived')){ %>
        Unarchive list
      <% } else { %>
        Archive list
      <% }; %>
    </button>

    <button class="delete-list">Delete list</button>

  </menu>
</div>